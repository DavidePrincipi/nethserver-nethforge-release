[nethforge]
name=NethForge modules - $basearch
baseurl=http://pulp.nethserver.org/nethserver/$releasever/nethforge/$basearch/
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-NETHFORGE-6
enabled=1
gpgcheck=1
enablegroups=1

[nethforge-testing]
name=NethForge modules - $basearch - Testing
baseurl=http://pulp.nethserver.org/nethserver/$releasever/nethforge-testing/$basearch/
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-NETHFORGE-6
enabled=0
gpgcheck=1
enablegroups=1
